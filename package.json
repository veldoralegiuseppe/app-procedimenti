{
  "name": "app-procedimenti",
  "productName": "app-procedimenti",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main/main.bundle.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "release": "standard-version && node integrations/create-jira-version.js && node integrations/publish-confluence-page.js",
    "prepare": "husky",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "cy:open": "cypress open",
    "cy:run": "ELECTRON_IS_TEST=true start-server-and-test start http://localhost:3000 cy:open",
    "cy:run:headless": "cypress run --headless --spec"
  },
  "keywords": [],
  "author": {
    "name": "veldoralegiuseppe",
    "email": "veldorale.giuseppe@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.25.8",
    "@babel/preset-env": "^7.25.8",
    "@babel/preset-react": "^7.25.7",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@cypress/webpack-dev-server": "^3.11.0",
    "@cypress/webpack-preprocessor": "^6.0.2",
    "@electron-forge/cli": "^7.2.0",
    "@electron-forge/maker-deb": "^7.2.0",
    "@electron-forge/maker-rpm": "^7.2.0",
    "@electron-forge/maker-squirrel": "^7.2.0",
    "@electron-forge/maker-zip": "^7.2.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.2.0",
    "@electron-forge/plugin-webpack": "^7.2.0",
    "@testing-library/cypress": "^10.0.2",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@vercel/webpack-asset-relocator-loader": "^1.2.4",
    "@welldone-software/why-did-you-render": "^8.0.3",
    "axios": "^1.7.7",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.2.1",
    "babel-plugin-inline-react-svg": "^2.0.2",
    "babel-plugin-module-resolver": "^5.0.2",
    "css-loader": "^6.10.0",
    "cypress": "^13.15.0",
    "dotenv": "^16.4.5",
    "electron": "28.2.0",
    "electron-devtools-installer": "^3.2.0",
    "file-loader": "^6.2.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "node-loader": "^2.0.0",
    "sass": "^1.79.4",
    "sass-loader": "^16.0.2",
    "simple-git": "^3.27.0",
    "standard-version": "^9.5.0",
    "start-server-and-test": "^2.0.8",
    "style-loader": "^3.3.4"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@fontsource/public-sans": "^5.0.16",
    "@fontsource/roboto": "^5.0.8",
    "@fontsource/titillium-web": "^5.0.19",
    "@marketto/belfiore-connector-embedded": "^1.2.1",
    "@marketto/codice-fiscale-utils": "^2.1.2",
    "@mui/icons-material": "^6.1.2",
    "@mui/material": "^6.1.2",
    "@mui/system": "^6.1.2",
    "@mui/x-charts": "^7.18.0",
    "@mui/x-date-pickers": "^7.19.0",
    "dayjs": "^1.11.10",
    "electron-squirrel-startup": "^1.0.0",
    "fs": "^0.0.1-security",
    "immer": "^10.1.1",
    "jquery": "^3.7.1",
    "lodash": "^4.17.21",
    "mathjs": "^13.2.0",
    "path-browserify": "^1.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zustand": "^5.0.1"
  },
  "config": {
    "forge": {
      "plugins": [
        {
          "name": "@electron-forge/plugin-webpack",
          "config": {
            "mainConfig": "./webpack.main.config.js",
            "devContentSecurityPolicy": "connect-src 'self' * 'unsafe-eval'",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "name": "main_window",
                  "html": "./src/index.html",
                  "js": "./src/renderer.js",
                  "preload": {
                    "js": "./src/preload.js"
                  }
                }
              ]
            }
          }
        }
      ]
    }
  }
}
