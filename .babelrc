{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "@assets": "./src/assets",
          "@components": "./src/components",
          "@context": "./src/context",
          "@pages": "./src/pages"
        }
      }
    ],
    "inline-react-svg"
  ]
}
