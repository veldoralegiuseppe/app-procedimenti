{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "@assets": "./src/assets",
          "@pages": "./src/renderer/pages",
          "@utils": "./src/renderer/shared/utils",
          "@ui-shared": "./src/renderer/shared",
          "@features": "./src/renderer/features",
          "@shared": "./src/shared",
        }
      }
    ],
    "inline-react-svg"
  ]
}
